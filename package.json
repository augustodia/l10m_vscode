{
  "name": "flutter-l10m",
  "displayName": "Flutter l10m",
  "description": "Flutter module localization extension",
  "version": "2.0.0",
  "publisher": "your-publisher-name",
  "repository": {
    "type": "git",
    "url": "https://github.com/augustodia/flutter-l10m.git"
  },
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Localization"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "Flutter l10m",
      "properties": {
        "l10m.command": {
          "type": "string",
          "default": "dart run l10m",
          "description": "Custom command to execute when a .arb file is changed"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "package": "vsce package",
    "pretest": "yarn run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.87.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9"
  },
  "keywords": [
    "flutter",
    "localization",
    "l10n",
    "i18n",
    "dart",
    "translation"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#185abd",
    "theme": "dark"
  },
  "badges": [
    {
      "url": "https://img.shields.io/github/stars/augustodia/flutter-l10m.svg",
      "href": "https://github.com/augustodia/flutter-l10m",
      "description": "GitHub Stars"
    }
  ],
  "bugs": {
    "url": "https://github.com/augustodia/flutter-l10m/issues"
  },
  "homepage": "https://github.com/augustodia/flutter-l10m#readme",
  "license": "MIT"
}
